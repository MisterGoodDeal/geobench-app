{
  "name": "geobench",
  "version": "1.3.3",
  "private": true,
  "scripts": {
    "link": "cd ios && pod install && cd ..",
    "android": "npx react-native run-android",
    "build": "cd android && ./gradlew bundleRelease && cd ..",
    "ios": "npx react-native run-ios --simulator='iPhone 11'",
    "start": "npx react-native start",
    "open-link": "npx uri-scheme open geobench://app/register --android",
    "android-bundle": "react-native bundle --platform android --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --dev false --reset-cache --assets-dest android/app/src/main/res/",
    "test": "jest",
    "mobile": "npm run ios && npm run android",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "rnpm": {
    "assets": [
      "assets/fonts"
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.5",
    "@react-native-community/cli": "^5.0.1",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-picker/picker": "^1.16.3",
    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "@reduxjs/toolkit": "^1.6.0",
    "check-password-strength": "^2.0.3",
    "email-validator": "^2.0.4",
    "form-data": "^3.0.1",
    "node-fetch": "^2.6.1",
    "react": "17.0.1",
    "react-native": "0.64.2",
    "react-native-appearance": "^0.3.4",
    "react-native-eject": "^0.1.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-picker": "^4.0.4",
    "react-native-localize": "^2.1.1",
    "react-native-map-clustering": "^3.4.2",
    "react-native-maps": "0.27.1",
    "react-native-picker-select": "^8.0.4",
    "react-native-ratings": "^8.1.0",
    "react-native-reanimated": "^2.2.0",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-spinkit": "^1.5.1",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-toast-message": "^1.4.9",
    "react-navigation": "^4.4.3",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "toggle-switch-react-native": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/node-fetch": "^2.5.10",
    "@types/react-native": "^0.64.5",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.1.2",
    "react-test-renderer": "17.0.1",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
